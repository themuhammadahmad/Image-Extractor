{
  "manifest_version": 3,
  "name": "Image Extractor",
  "version": "1.0",
  "permissions": [
    "scripting",
    "storage",
    "activeTab",
    "downloads",
    "cookies"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title" : "Click Me",
    "default_icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png"
  }
}
